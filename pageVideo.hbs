{{!< default}} <style>

    .feed-header-wrapper {
    align-items: flex-end;
    border-bottom: 0px solid var(--light-gray-color) !important;
    display: flex;
    justify-content: space-between;
    padding-bottom: 0rem;
    }
    .line_outr{
    display: flex;
    justify-content:start;
    align-items: center;
    gap: 10px;
    max-width: 200px;
    width: 200px;
    padding-top: 10px;
    }
    .line_inr{
    width: 30%;
    height: 4px;
    background-color: #02cce6;
    }
    .line_inr_one{
    width: 60%;
    }
    .related-wrapper {
    background-color: #192835;
    margin-top: 80px;
    padding: 0 ;
    }
    </style>
    <main class="site-main">

        <header class="feed-header gh-canvas">
            {{#if @custom.feed_title}}
            <div class="feed-header-wrapper">
                <h2 class="feed-header-title">{{@custom.feed_title}}</h2>
            </div>
            <span class="line_outr">
                <span class="line_inr line_inr_one"></span>
                <span class="line_inr"></span>
            </span>
            {{/if}}
        </header>
        {{!-- filter="tag:videos" --}}
        {{!-- {{#get "posts" filter="tag:videos" order="published_at desc" include ="tags" as |posts |}} --}}

        {{#if 'posts'}}
        <section class="related-wrapper gh-canvas">

            <div class="post-feed expanded related-feed">
                {{#foreach posts}}
                {{> "loop"}}
                {{/foreach}}
            </div>
        </section>
        {{/if}}
        {{pagination}}
        {{!-- {{/get}} --}}



    </main>
